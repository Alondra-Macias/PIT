# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: 1PIT.axf

# Tool invocations
1PIT.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -Xlinker -Map="1PIT.map" -Xlinker --gc-sections -Xlinker -print-memory-usage -Xlinker --sort-section=alignment -mcpu=cortex-m4 -mfpu=fpv4-sp-d16 -mfloat-abi=hard -mthumb -T 1PIT_Debug.ld -o "1PIT.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(EXECUTABLES)$(OBJS)$(C_DEPS) 1PIT.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-size "1PIT.axf"; # arm-none-eabi-objcopy -v -O binary "1PIT.axf" "1PIT.bin" ; # checksum -p MK64FN1M0xxx12 -d "1PIT.bin";
	-@echo ' '

